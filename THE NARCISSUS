#include<stdio.h>

int main()
{
	//水仙花数
	//三位数153 = 1^3 + 5^3 +3^3 
	//要求：输入N位数,输出所有 
	//scanf("%d",&n);
	
//	3 >> 100
//	1
//	0
//	0
//	100 == 1^3 + 0^3 + 0^3
//	100>>101 
   printf("计算n位数中的水仙花数\n"); 

	int n;//搞定位数 
	scanf("%d",&n);
	int digit = 1,i = 1;
	while (i < n){
		digit *= 10;
		i++;
	}
	
	int x, k = 0,a,sum = 0,t,e,m = 1,p=1;//搞定每一位并加起来
	x = digit;
	while(x < digit*10){//现在我们有了一个确定的数 ,提取并相乘 
			t = x;//保证x不变 
			e = digit;//保证digit不变
			while(k < n){
				a = t / e;
				t = t % e;
				e /= 10;
				p = a;
				while(m < n-1){
					a *= p;
					m++;
				}
				sum += a;
				m = 0;
				k++; 
			} 
			k = 0;
		if (sum == x){
			printf("%d ",sum);
		}
		x++;
		//x = 153;
		sum = 0;
	}
	//检验printf("%d",);
	
	
	
	return 0;
}
